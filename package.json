{
  "scripts": {
    "start": "npx parcel serve -p 8080 src/index.html",
    "build": "npx parcel build --experimental-scope-hoisting src/index.html && cp -r ./.well-known ./dist",
    "deploy": "npx firebase deploy --only hosting:onetaphello-landing",
